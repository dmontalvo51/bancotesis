<mapper namespace="pe.edu.unmsm.integracion.dao.PerfilMapper">
	
	
	<select id="cargarPerfiles" parameterType="java.lang.String" resultType="pe.edu.unmsm.negocio.modelo.DatoMaestro">
		SELECT p.PER_ID as codigo, p.PER_DESCRIPCION as descripcion FROM tesis.perfil p
	</select>


</mapper>